!!! 5
%html
  %head
    %title
      = yield :title
    = stylesheet_link_tag    "application", :media => "all"
    = javascript_include_tag "application"
    = csrf_meta_tags
  %body
    = render 'shared/navbar'
    .container
      - unless flash.empty?
        - flash.each do |key, val|
          %p{ :class => "#{key} well" }
            = val
      = yield
